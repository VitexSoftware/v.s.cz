{
    "name": "vitexsoftware/vscz",
    "description": "VitexSoftware homepage",
    "version": "0.202.6",
    "authors": [
        {
            "name": "vitex",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "vendor-dir": "/var/lib/vitexsoftware.cz/"
    },
    "autoload": {
        "psr-4": {
            "VSCZ\\": "classes/",
            "VSCZ\\ui\\": "classes/ui/"
        }
    },
    "require": {
        "php": ">= 7.1",
        "league/commonmark": "*",
        "spoje.net/flexibee": "*",
        "vitexsoftware/ease-core": "dev-master",
        "vitexsoftware/ease-twbootstrap4-widgets": "dev-master",
        "vitexsoftware/ease-fluentpdo": "dev-master"

    },
    "repositories": [
        {
            "type": "path",
            "url": "/usr/share/php/Ease",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "/usr/share/php/FlexiPeeHP",
            "options": {
                "symlink": true
            }
        }
    ]
}

{
    "name": "vitexsoftware/vscz",
    "description": "VitexSoftware homepage",
    "version": "0.202.6",
    "authors": [
        {
            "name": "vitex",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "vendor-dir": "/var/lib/vitexsoftware.cz/"
    },
    "autoload": {
        "psr-4": {
            "VSCZ\\": "classes/",
            "VSCZ\\ui\\": "classes/ui/"
        }
    },
    "require": {
        "ext-fileinfo": "*",
        "league/commonmark": "*",
        "deb/abraflexi": "*",
        "vitexsoftware/ease-core": "*",
        "vitexsoftware/ease-twbootstrap4": "*",
        "vitexsoftware/ease-twbootstrap4-widgets": "*",
        "vitexsoftware/ease-fluentpdo": "*"

    },
    "repositories": [
        {
            "type": "path",
            "url": "/usr/share/php/EaseCore",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "/usr/share/php/AbraFlexi",
            "options": {
                "symlink": true
            }
        }
    ]
}
